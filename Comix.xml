<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/
    ..\..\..\FrameXML\UI.xsd">

	<Script file="Comix.lua"/>

	<Frame name="Comix_Frame" hidden="false">
		<Scripts>
	      		<OnLoad>
	       			Comix_OnLoad();
	      		</OnLoad>
	      		<OnEvent>
	        		Comix_OnEvent(event, arg1, arg2, arg3);
	      		</OnEvent>
		        <OnUpdate>
		          Comix_OnUpdate(arg1)
		        </OnUpdate>  
		</Scripts>
  	</Frame>
</Ui>
